--------------------------------------------------------
public class Krafthermadur_D : Dressar
    {
        //breytur
        private int health;
        private int strength;
        private int weapon;

        public int Health
        {
            get
            {
                return health;
            }
            set
            {
                if (value < 10 || value > 15)//Set if svo það hermaðurinn getur ekki fengið hærra en 15 í lífi og lægra en 10
                {
                    health = 0;
                }
                else
                {
                    health = value;
                }
            }
        }//health end
        public int Strength
        {
            get
            {
                return strength;
            }
            set
            {
                if (value < 0 || value > 10)//Set if svo það hermaðurinn getur ekki fengið hærra en 10 í strenght og lægra en 0
                {
                    strength = 0;
                }
                else
                {
                    value = strength;
                }
            }
        }//strength 
        public int Weapon
        {
            get
            {
                return weapon;
            }
            set
            {
                if (value < 1 || value > 3)//Hef if hérna svo hann geti bara fengið vopn frá 1-3
                {
                    weapon = 1;
                }
                else
                {
                    value = weapon;
                }
            }
        }//weapon end
        public Krafthermadur_D(int lif, int styrkur, int vopn)
            :base(lif, styrkur, vopn)
        {
            health = lif;
            strength = styrkur;
            weapon = vopn;
        }//KrafthermadurD end
        public override string ToString()
        {
            return string.Format("Halló ég er Krafthermaður og hef " + Health + " líf og " + Strength + " Styrkleika og þetta " + Weapon + " vopn");
        }
    }
