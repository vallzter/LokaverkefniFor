public class Vellir : GrunnHermaður
    {
        //breytur
        private int health;
        private int strength;
        private int weapon;

        public int Health
        {
            get
            {
                return health;
            }
            set
            {
                if (value < 0 || value > 5)//Set if svo það hermaðurinn getur ekki fengið hærra en 5 í lífi og lægra en 0
                {
                    health = 0;
                }
                else
                {
                    health = value;
                }
            }
        }//health end
        public int Strength
        {
            get
            {
                return strength;
            }
            set
            {
                if (value < 0 || value > 5)//Set if svo það hermaðurinn getur ekki fengið hærra en 5 í strenght og lægra en 0
                {
                    strength = 0;
                }
                else
                {
                    value = strength;
                }
            }
        }//strength 
        public int Weapon
        {
            get
            {
                return weapon;
            }
            set
            {
                if (value < 1 || value > 3)//Hef if hérna svo hann geti bara fengið vopn frá 1-3
                {
                    weapon = 1;
                }
                else
                {
                    value = weapon;
                }
            }
        }//weapon end
        public Vellir(int lif, int styrkur, int vopn)
            :base(lif, styrkur, vopn)
        {
            health = lif;
            strength = styrkur;
            weapon = vopn;
        }//Vellir end
        public override string ToString()
        {
            return base.ToString();
        }
    }

     
 
    
    
 
